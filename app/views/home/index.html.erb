<div class="container">
  <div class="links"></div>
  <div class="notes"></div>
  <div class="utility">
    <% if user_signed_in? %>
      <%= select_tag(:campaign_id, options_for_select(current_user.campaigns.map { |c| [c.name,c.id] } ) )%>
    <% end %>
    <!---testing-->
    <ul>
      <% @campaigns.each do |campaign| %>
        <li>
          <%= link_to campaign.name, campaign %>
        </li>
      <% end %>
  </div>
</div>